CREATE SEQUENCE people_seq
INCREMENT BY 1
START WITH 1;

CREATE SEQUENCE users_seq
INCREMENT BY 1
START WITH 1;

CREATE SEQUENCE workers_seq
INCREMENT BY 1
START WITH 1;

CREATE SEQUENCE customers_seq
INCREMENT BY 1
START WITH 1;

/*Barcode: default 15 digits*/
CREATE SEQUENCE bar_code_seq
INCREMENT BY 1
START WITH 100000000000000;
